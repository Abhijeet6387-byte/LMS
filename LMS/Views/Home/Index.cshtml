        <div class="row register py-5" id="register">
            <div class="text-center" style="line-height:20px !important;">
                <p class="fs-3">
                    Register
                    <b class="txt-color2">
                        Here 
                        <i class="fa-solid fa-user-plus"></i>
                    </b>
                </p>
                <span style="border-bottom:2px solid var(--color2);padding-bottom:3px;font-size:16px;font-weight:500">Sign Up for Your Learning Journey</span>
            </div>
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <form action="/home/saveuser" method="post" enctype="multipart/form-data">
                    <div class="row">
                        <div class="col-sm-12 p-4 mt-5 form">
                            <div class="row">
                                <div class="col-sm-6">
                                    Name<br /><input type="text"
                                                     placeholder="Name.." name="txt_name"
                                                     class="form-control mb-3 shadow bg-body-tertiary border-0" />
                                </div>
                                <div class="col-sm-6">

                                    Email<br /><input type="email"
                                                      placeholder="Email.." name="txt_email"
                                                      class="form-control mb-3 shadow bg-body-tertiary border-0" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    Mobile<br /><input type="number"
                                                       placeholder="Number.." name="txt_mobno"
                                                       class="form-control mb-3 shadow bg-body-tertiary border-0" />
                                </div>
                                <div class="col-sm-6">

                                    Profile Picture<br /><input type="file" name="file_profile"
                                                                class="form-control mb-3 py-3 shadow bg-body-tertiary border-0" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    Course and Branch<br />
                                    <select class="form-control mb-3 shadow bg-body-tertiary border-0" name="ddl_course">
                                        <option>--Select Course--</option>
                                        <option>Diploma (CS/IT)</option>
                                    </select>
                                </div>
                                <div class="col-sm-6">

                                    Year<br />
                                    <select class="form-control mb-3 shadow bg-body-tertiary border-0" name="ddl_year">
                                        <option>--Select Year --</option>
                                        <option>1st Year</option>
                                        <option>2nd Year</option>
                                        <option>3rd Year</option>
                                        <option>4th Year</option>
                                        <option>Passout</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">

                                    Select Batch<br />
                                    <select class="form-control mb-3 shadow bg-body-tertiary border-0" name="ddl_batch">
                                        <option>-- Select Batch--</option>
                                        @if (ViewBag.tb1.Rows.Count > 0)
                                        {
                                            foreach (var row in ViewBag.tb1.Rows)
                                            {
                                                <option value="@row[0]">@row[1]</option>
                                            }
                                        }

                                    </select>
                                </div>
                                <div class="col-sm-6">

                                    Password<br /><input type="password"
                                                         placeholder="Placeholder.." name="txt_passwd"
                                                         class="form-control mb-3 shadow bg-body-tertiary border-0" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <br /><input type="submit"
                                                 class="form-control mb-3 w-25 border-0" style="margin-left:37%;background-color:lightgreen;color:var(--color3);" />
                                </div>

                            </div>
                        </div>

                    </div>
                </form>
            </div>
