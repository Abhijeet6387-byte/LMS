    <style>
        :root {
            --color1: #00447e;
            --color2: orange;
            --color3: black;
        }
        body{
            overflow-x:hidden;
        }

        .bg-mycolor1 {
            background: var(--color1);
        }

        .bg-mycolor2 {
            background: var(--color2);
        }

        .bg-mycolor3 {
            background: var(--color3);
        }

        .txt-color1 {
            color: var(--color1) !important;
        }


        .txt-color2 {
            color: var(--color2) !important;
        }

        .txt-color3 {
            color: var(--color3) !important;
        }

        .ic {
            border: 1px solid black;
            height: 20px;
            width: 20px;
            line-height: 20px;
            transition: all .6s;
        }

            .ic:hover {
                border: 1px solid var(--color1);
                color: var(--color1);
                transition: all .6s;
                transform: rotate(360deg);
            }

        .t1 {
            font-size: 14px;
            color: black;
            background:orange;

        }

            .t1 a {
                text-decoration: none;
                color: black;
            }

        #menu {
            background: transparent !important;
            
        }

        .slider {
            min-height: 500px;
            background-image: url('../../content/images/8271020_5514.jpg');
            padding:2px 0px 2px 0px;
            background-size: 100% 100%;
            background-position: center;
            animation: img 10s infinite;
        }
        @@keyframes img{
                 0%
        {
            background-image: url('../../content/images/7732666_5293.jpg');
        }
        20% {
            background-image: url('../../content/images/9175118_6461.jpg');
        }
            40% {
                background-image: url('../../content/images/img1/learning111.jpg');
            }
            60% {
                
                background-image: url('../../content/images/learning-education-academics-knowledge-concept.jpg');
            }
            80% {
                
                background-image: url('../../content/images/8642448_3967400.jpg');
            }
            100% {
                
                background-image: url('../../content/images/img1/learning080.jpg');
            }
             }

        .login {
            background: rgba(255,255,255,.2);
            margin-top: 8%;
            padding: 4%;
            backdrop-filter: blur(3px);
            color: white;
            border-radius: 10px;
        }

        .form-control {
            border-radius: 0px !important;
            height: 50px;
            margin-bottom: 10px;
        }

        .btn1 {
            height: 50px;
            width: 120px;
            border: none;
            color: white;
        }

        .sicon {
            height: 50px;
            width: 50px;
            background: var(--color2);
            text-align: center;
            line-height: 50px;
            border-radius: 50%;
            margin-bottom: 10px;
            font-size: 25px;
            color: white;
        }

        .abouttext {
            font-size: 18px;
            text-align: justify;
            margin: 0px 3px 0px 3px;
        }

        .faqText {
            font-size: 18px;
            text-align: justify;
            margin: 3% 5% 0% 5%;
        }
        .t2{
            background:transparent;
        }
        .s1 {
            box-shadow: 0px 0px 8px var(--color1);
            font-size: 15px;
            line-height: 27px;
            border-radius: 10px;
            transform:scale(.8);
            transition: all ease 1s;
        } 
        .s1:hover{
            transform:scale(1.1);
            transition:all ease 1s;
        }
        .form {
            box-shadow: 0px 0px 8px var(--color1);
            border-radius: 10px;
            background: #deeef0;
            
        }
        .f1{
            box-shadow:0px 0px 2px var(--color1);
            
        }
        .f1:focus{
            border-color:var(--color1);
            outline:none;
        }
        .cinfo {
            
            transition: transform ease 2s;
        }
        .cinfo:hover{
            transform:translateY(-25px);
            transition:transform ease 2s;
        }
        .ficon {
            transition: all ease 2s;
        }
        .ficon:hover{
            border-bottom:2px solid var(--color2);
            transition:all ease 1s;
        }
    </style>
</head>
